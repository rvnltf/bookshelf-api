{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "indent":"off",
        "max-len":"off",
        "object-curly-newline":"off",
        "space-infix-ops":"off",
        "arrow-spacing":"off",
        "comma-dangle":"off",
        "eol-last":"off",
        
    }
}
